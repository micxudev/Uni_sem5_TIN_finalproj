    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-tsconfig-paths": "^6.0.3"
  },
  "scripts": {
    "dev:backend": "ts-node --files -r tsconfig-paths/register apps/backend/src/server.ts",
    "dev:frontend": "vite --config apps/frontend/vite.config.ts",
    "build:backend": "npx tsc && tsc-alias",
    "build:frontend": "vite build --config apps/frontend/vite.config.ts",
    "build": "npm run build:backend && npm run build:frontend",
    "start": "node dist/apps/backend/src/server.js",
    "preview:frontend": "vite preview --config apps/frontend/vite.config.ts"
  }
